<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin certificationLevel="None" displayName="Example Plugin" minSystemVersion="8.3" name="ExamplePlugin" version="1.0">
